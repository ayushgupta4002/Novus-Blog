
<%- include("partials/header"); -%>
    <h1>Compose</h1>
    <form class="" action="/composepost" method="post">
      <div class="form-group">
        <label>Title</label>
        <input class="form-control" type="text" name="postTitle">

        <label>image for main page</label>
        <input class="form-control" type="text" name="imglink">

        <label>image for blog page</label>
        <input class="form-control" type="text" name="imglinkblog">

        <label>readtime</label>
        <input class="form-control" type="text" name="rdtime">

        <label>Post</label>
        <textarea class="form-control" name="postBody" rows="5" cols="30"></textarea>

      </div>
      <button class="btn btn-primary" type="submit" name="button">Publish</button>
    </form>

<%- include("partials/footer"); -%>
